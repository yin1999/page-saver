{
	"manifest_version": 3,
	"name": "page saver",
	"description": "save pages",
	"version": "0.06",
	"permissions": [
		"pageCapture",
		"commands",
		"tabs",
		"activeTab",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	},
	"options_page": "options/options.html",
	"icons": {
		"128": "images/download-on.png"
	},
	"commands": {
		"save-page": {
			"suggested_key": {
				"default": "Ctrl+Shift+Y",
				"mac": "Command+Shift+Y"
			},
			"description": "Save the current page."
		}
	},
	"action": {}
}
